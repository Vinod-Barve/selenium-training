<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Parameters Example" parallel="tests" thread-count="2">

  <parameter name="browserName"  value="Chrome"/>
  <parameter name="browserVersion"  value="1.2.0"/>

  <test name="Chrome Test">
    <classes>
      <class name="testng.ParametersAndDataProviderExample"/>
    </classes>
  </test>
  
  <test name="Firefox Test">
  <parameter name="browserName"  value="FireFox"/>
  <parameter name="browserVersion"  value="1.2.3"/>
  
  	<classes>
      <class name="testng.ParametersAndDataProviderExample"/>
    </classes>
  </test>
  
</suite>
