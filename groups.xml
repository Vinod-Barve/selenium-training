<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Groups Example">

	<test name="Groups Example">
		
		<groups>
			<run>
				<include name="smoke" />
				<include name="regression" />
				<exclude name="funcTest" />			
			</run>
		</groups>

		<packages>
			<package name="testng" />
		</packages>

	</test>
</suite> <!-- Suite -->
